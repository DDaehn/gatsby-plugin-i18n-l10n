{
  "name": "gatsby-plugin-i18n-l10n",
  "version": "4.4.1",
  "description": "Providing i18n and l10n to Gatsby with react-intl and react-helmet. Besides translating pages and labels, you can also translate the slugs and paths and still link between translated sibling pages.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-i18n-l10n",
    "i18n",
    "l10n",
    "intl",
    "internationalization",
    "translate",
    "react-intl",
    "react-helmet"
  ],
  "homepage": "https://github.com/openscript-ch/gatsby-plugin-i18n-l10n",
  "author": "openscript",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "check:format": "eslint . --max-warnings 0",
    "check:types": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "release:major": "standard-version --release-as major && git push --follow-tags",
    "release:minor": "standard-version --release-as minor && git push --follow-tags",
    "release:patch": "standard-version --release-as patch && git push --follow-tags",
    "release:create-github-release": "conventional-github-releaser -p angular",
    "test": "jest"
  },
  "peerDependencies": {
    "gatsby": "^4.x",
    "gatsby-source-filesystem": "^4.x",
    "react-helmet": "^6.1.x",
    "react-intl": "^5.20.x"
  },
  "peerDependenciesMeta": {
    "gatsby-source-filesystem": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/react-helmet": "^6.1.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-preset-gatsby": "^2.5.2",
    "conventional-github-releaser": "^3.1.5",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.5",
    "gatsby": "^4.x",
    "gatsby-source-filesystem": "^4.x",
    "react-helmet": "^6.1.x",
    "react-intl": "^5.20.x"
  },
  "dependencies": {
    "limax": "^3.0.0",
    "path-browserify": "^1.0.1"
  }
}
